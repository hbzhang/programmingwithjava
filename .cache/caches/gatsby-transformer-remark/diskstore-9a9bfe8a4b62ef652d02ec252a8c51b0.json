f __init__(self, name, age):\n    self.name = name\n    self.age = age\n\np1 = Person(&quot;John&quot;, 36)\n\nprint(p1.name)\nprint(p1.age)\n\n\nclass Person:\n  \n  def __init__(self, name, age):\n    self.age = age\n    self.name = name\n  \n  def stroperate(self, name, age):\n    return name+&quot; :&quot;+ str(age)\n  \n  @classmethod\n  def classmethodop(cls,name,age):\n    return age**3\n  \n  @staticmethod\n  def staticmethodop(name):\n    return name + &quot; &quot; + name\n  \np = Person(&quot;ff&quot;, 13)\nprint(p.name)\nprint(p.age)\nprint(p.stroperate(p.name, p.age))\nprint(Person.classmethodop(&quot;ff&quot;,13))\nprint(Person.staticmethodop(&quot;FFF&quot;))</code></pre></div>","position":{"start":{"line":200,"column":1,"offset":3542},"end":{"line":240,"column":4,"offset":4186}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{"id":"python-class-1"},"children":[{"type":"element","tagName":"a","properties":{"href":"#python-class-1","aria-label":"python class 1 permalink","class":"anchor"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg>"}]},{"type":"text","value":"Python Class 1","position":{"start":{"line":245,"column":5,"offset":4195},"end":{"line":245,"column":19,"offset":4209}}}],"position":{"start":{"line":245,"column":1,"offset":4191},"end":{"line":245,"column":19,"offset":4209}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"gatsby-highlight\" data-language=\"text\"><pre class=\"language-text\"><code class=\"language-text\">class Person:\n  \n  def __init__(self, name, age):\n    self.age = age\n    self.name = name\n    \n  \n  \n  def stroperate(self):\n    if self.age &gt;50:\n      self.howold = &quot;middle aged&quot;\n    elif self.age &gt;30 and self.age &lt;49:\n      self.howold = &quot;young aged&quot;\n    elif self.age &lt;20 and self.age &gt;10:\n      self.howold = &quot;Teenage&quot;\n    return self.howold \n  \n  def getpersonname(self):\n    return self.name\n  \n  @classmethod\n  def classmethodop(cls,name,age):\n    return age**3\n  \n  @staticmethod\n  def staticmethodop(name):\n    return name + &quot; &quot; + name\n  \np = Person(&quot;ff&quot;, 13)\nprint(p.name)\nprint(p.age)\nprint(p.stroperate())\nprint(Person.classmethodop(&quot;ff&quot;,13))\nprint(Person.staticmethodop(&quot;FFF&quot;))\n\n\n\nclass Student(Person):\n  \n  def __init__(self,name,age,tuition):\n    self.name = name\n    self.age = age\n    self.tuition = tuition\n  \n  \n  def getpersonname(self):\n    return self.name\n  \n  def gettuition(self):\n    return self.tuition\n\n\np = Person(&#39;gg&#39;, 255)\n\nprint(p.getpersonname())\n\nst = Student(&#39;asdasd&#39;,1233,121231231)\nprint(st.getpersonname())\nprint(st.gettuition())</code></pre></div>","position":{"start":{"line":247,"column":1,"offset":4211},"end":{"line":309,"column":4,"offset":5289}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{"id":"python-exception"},"children":[{"type":"element","tagName":"a","properties":{"href":"#python-exception","aria-label":"python exception permalink","class":"anchor"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5