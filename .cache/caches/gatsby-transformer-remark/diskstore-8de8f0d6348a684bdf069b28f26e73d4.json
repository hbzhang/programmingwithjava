3\n  \n  @staticmethod\n  def staticmethodop(name):\n    return name + &quot; &quot; + name\n  \np = Person(&quot;ff&quot;, 13)\nprint(p.name)\nprint(p.age)\nprint(p.stroperate(p.name, p.age))\nprint(Person.classmethodop(&quot;ff&quot;,13))\nprint(Person.staticmethodop(&quot;FFF&quot;))</code></pre></div>","position":{"start":{"line":198,"column":1,"offset":3179},"end":{"line":238,"column":4,"offset":3823}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{"id":"python-class-1"},"children":[{"type":"element","tagName":"a","properties":{"href":"#python-class-1","aria-label":"python class 1 permalink","class":"anchor"},"children":[{"type":"raw","value":"<svg aria-hidden=\"true\" focusable=\"false\" height=\"16\" version=\"1.1\" viewBox=\"0 0 16 16\" width=\"16\"><path fill-rule=\"evenodd\" d=\"M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z\"></path></svg>"}]},{"type":"text","value":"Python Class 1","position":{"start":{"line":243,"column":5,"offset":3832},"end":{"line":243,"column":19,"offset":3846}}}],"position":{"start":{"line":243,"column":1,"offset":3828},"end":{"line":243,"column":19,"offset":3846}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"gatsby-highlight\" data-language=\"text\"><pre class=\"language-text\"><code class=\"language-text\">class Person:\n  \n  def __init__(self, name, age):\n    self.age = age\n    self.name = name\n    \n  \n  \n  def stroperate(self):\n    if self.age &gt;50:\n      self.howold = &quot;middle aged&quot;\n    elif self.age &gt;30 and self.age &lt;49:\n      self.howold = &quot;young aged&quot;\n    elif self.age &lt;20 and self.age &gt;10:\n      self.howold = &quot;Teenage&quot;\n    return self.