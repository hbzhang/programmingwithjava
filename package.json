{
  "name": "docs",
  "version": "1.0.0",
  "description": "Documentation for the Ghost publishing platform",
  "repository": "git@github.com:TryGhost/docs.git",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "deploy": "gatsby-plugin-s3 deploy",
    "serve": "gatsby build && NODE_ENV=production gatsby serve",
    "build": "gatsby build",
    "dev": "netlify dev",
    "lint": "eslint . --ext .js --cache",
    "clean": "gatsby clean",
    "posttest": "yarn lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:8000 cy:run",
    "test": "NODE_ENV=testing mocha -- $(find test -name '*.test.js') && yarn test:e2e:ci",
    "preship": "yarn test",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi",
    "master": "git pull -r origin master --autostash && cd content/api/v2 && git pull -r origin master --autostash && cd ../../../ && yarn"
  },
  "devDependencies": {
    "@tryghost/admin-api": "^0.4.2",
    "@tryghost/content-api": "^1.2.1",
    "algoliasearch": "3.32.1",
    "autoprefixer": "9.5.1",
    "cssnano": "4.1.10",
    "cypress": "3.2.0",
    "cypress-testing-library": "3.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-ghost": "0.2.0",
    "eslint-plugin-react": "7.12.4",
    "ghost-spirit": "0.0.50",
    "mocha": "6.1.4",
    "postcss-color-mod-function": "3.0.3",
    "postcss-css-variables": "0.12.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "8.0.10",
    "postcss-easy-import": "3.0.0",
    "should": "13.2.3",
    "sinon": "7.3.2",
    "start-server-and-test": "1.9.0",
    "surge": "latest",
    "git-scripts": "0.2.1"
  },
   "git": { "scripts": { "pre-push": "surge --project public/ --domain judicious-cub.surge.sh" } },
  "dependencies": {
    "@tryghost/helpers": "^1.1.3",
    "@tryghost/helpers-gatsby": "1.0.5",
    "algolia-html-extractor": "0.0.1",
    "gatsby": "2.3.34",
    "gatsby-image": "2.0.41",
    "gatsby-plugin-advanced-sitemap": "1.0.5",
    "gatsby-plugin-algolia": "0.3.0",
    "gatsby-plugin-catch-links": "2.0.13",
    "gatsby-plugin-force-trailing-slashes": "1.0.4",
    "gatsby-plugin-manifest": "2.0.29",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-postcss": "2.0.7",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-react-svg": "2.1.1",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-s3": "^0.2.5",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-code-titles": "1.1.0-rc.0",
    "gatsby-remark-embed-snippet": "3.2.4",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.11",
    "gatsby-remark-prismjs": "3.2.8",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-source-ghost": "3.3.1",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.18",
    "gatsby-transformer-yaml": "2.1.12",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-instantsearch-dom": "5.5.0",
    "react-modal": "3.8.1",
    "tocbot": "4.6.0"
  }
}
